  <DataSets>
    <DataSet Name="isa_DataSet">
      <Query>
        <DataSourceName>IASSTG_BCGOV</DataSourceName>
        <QueryParameters>
          <QueryParameter Name=":Parid">
            <Value>=Parameters!parid.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
          <QueryParameter Name=":Jur">
            <Value>=Parameters!jur.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
          <QueryParameter Name=":Taxyr">
            <Value>=Parameters!taxyr.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT V.PARID, V.Jur, V.TaxYr, V.Tot01, PD.CITYNAME
  FROM ASMT V, PARDAT PD
 WHERE V.TaxYr = :taxyr
   AND V.TaxYr = PD.TAXYR
   AND V.PARID = :parid 
   AND V.PARID = PD.PARID
   AND V.Jur = :jur
   AND V.JUR = PD.JUR</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="PARID">
          <DataField>PARID</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="JUR">
          <DataField>JUR</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="TAXYR">
          <DataField>TAXYR</DataField>
          <rd:TypeName>System.Int16</rd:TypeName>
        </Field>
        <Field Name="TOT01">
          <DataField>TOT01</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="CITYNAME">
          <DataField>CITYNAME</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="ds_TaxYr">
      <Query>
        <DataSourceName>IASSTG_BCGOV</DataSourceName>
        <CommandText>SELECT Distinct TaxYr
  FROM ASMT </CommandText>
